type InvitedUser {
  id: UUID!
  inviter: User!
  invitee: User!
  createdAt: Time
  updatedAt: Time
}

type InvitedUsersEdge {
  cursor: String!
  node: InvitedUser
}

type InvitedUsers {
  totalCount: TotalCount!
  edges: [InvitedUsersEdge]!
  pageInfo: PageInfo!
}

extend type Query {
  getInvitedUsers(pageInfoInput: PageInfoInput): InvitedUsers
}
