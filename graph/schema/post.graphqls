type Post {
  id: ID!
  user: User!
  content: String!
  status: Int
  createdAt: Time!
  updatedAt: Time!
}

input addPostInput {
  content: String!
  status: Int
}

extend type Query {
  getUserPostsByUsername(username: String!): [Post]!
}

extend type Mutation {
  addPost(input: addPostInput!): Post!
}
