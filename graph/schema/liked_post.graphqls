type LikedPost {
  id: ID!
  user: User!
  post: Post!
  createdAt: Time
  updatedAt: Time
}

type LikedPosts {
  likedPosts: [LikedPost]
  pagination: Pagination
}

extend type Query {
  getLikedPostsByPostId(postId: String!, input: PaginationInput): LikedPosts
}

extend type Mutation {
  likePost(postId: ID!): Post
}
