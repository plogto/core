type LikedPost {
  id: ID!
  user: User!
  post: Post!
  createdAt: Time
  updatedAt: Time
}

type LikedPostsEdge {
  cursor: String!
  node: LikedPost
}

type LikedPosts {
  totalCount: Int
  edges: [LikedPostsEdge]
  pageInfo: PageInfo!
}


extend type Query {
  getLikedPostsByPostId(postId: String!, pageInfoInput: PageInfoInput): LikedPosts
}

extend type Mutation {
  likePost(postId: ID!): Post
}
