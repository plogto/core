scalar Time
scalar TotalCount
scalar UUID

type PageInfo {
  endCursor: String!
  hasNextPage: Boolean
}

input PageInfoInput {
  first: Int
  after: String
}

type Test {
  content: String
}

input TestInput {
  content: String
}

type Query {
  test(input: TestInput!): Test
}

type Mutation {
  test(input: TestInput!): Test
}

type Subscription {
  test(input: TestInput!): Test
}
